<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="PayslipListView.print_button" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <xpath expr="//t[@t-if='props.showButtons']" position="inside">
            <button type="button" class="btn btn-secondary o_button_print_payslip" t-on-click="onPrintClick">
                Print
            </button>
        </xpath>
    </t>
</templates>
