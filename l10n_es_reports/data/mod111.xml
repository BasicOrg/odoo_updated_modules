                                        <field name="subformula">editable;rounding=2</field>
                                    </record>
                                </field>
                            </record>
                            <record id="mod_111_casilla_12" model="account.report.line">
                                <field name="name">[12] Importe de los ingresos a cuenta</field>
                                <field name="code">aeat_mod_111_12</field>
